-- ATF DDM by SleepyUncle
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Workspace = game:GetService("Workspace")
local TeleportService = game:GetService("TeleportService")
local player = Players.LocalPlayer
local mapIDs = {86392425558311, 80299472659017, 120958789962854, 76011326497329, 110577167676254} -- map IDs ‡∏ó‡∏µ‡πà‡∏à‡∏∞‡πÉ‡∏ä‡πâ‡∏ü‡∏≤‡∏£‡πå‡∏°
local safeZonePosition = CFrame.new(467.62384033203125, 315.87823486328125, -109.36079406738281)

-- üìå ‡∏£‡∏≠‡πÉ‡∏´‡πâ LocalPlayer ‡πÇ‡∏´‡∏•‡∏î‡πÄ‡∏™‡∏£‡πá‡∏à
while not player do
    player = game:GetService("Players").LocalPlayer
    wait()
end

-- üìå ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡∏û‡∏£‡πâ‡∏≠‡∏°‡∏Ç‡∏≠‡∏á‡∏ó‡∏∏‡∏Å‡∏≠‡∏¢‡πà‡∏≤‡∏á
local function checkPlayerAndPetReady()
    local character
    repeat
        character = player.Character or player.CharacterAdded:Wait()
        wait(0.1)
    until character:FindFirstChild("HumanoidRootPart") and character.HumanoidRootPart:FindFirstChild("pet1")
           and player:FindFirstChild("PlayerGui") and player.PlayerGui:FindFirstChild("CombatClient")
           and ReplicatedStorage:FindFirstChild("PartySystemRE")
    
    -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤ CombatClient ‡πÅ‡∏•‡∏∞ PartySystemRE ‡∏û‡∏£‡πâ‡∏≠‡∏°‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô‡∏´‡∏£‡∏∑‡∏≠‡πÑ‡∏°‡πà
    local combatClient = player.PlayerGui.CombatClient
    local partySystem = ReplicatedStorage.PartySystemRE
    if not combatClient or not partySystem then
        warn("‚ö† CombatClient ‡∏´‡∏£‡∏∑‡∏≠ PartySystemRE ‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡∏û‡∏£‡πâ‡∏≠‡∏°‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô!")
        return false
    end
    
    print("‚úî ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÅ‡∏•‡∏∞‡∏î‡∏¥‡∏à‡∏¥‡∏°‡πà‡∏≠‡∏ô‡∏û‡∏£‡πâ‡∏≠‡∏°‡πÅ‡∏•‡πâ‡∏ß! ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏£‡∏±‡∏ô‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå...")
    return true
end

-- üìå ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô‡∏Å‡∏≤‡∏£‡∏ó‡∏≥‡∏á‡∏≤‡∏ô
if checkPlayerAndPetReady() then
    -- ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏£‡∏±‡∏ô‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏ó‡∏∏‡∏Å‡∏≠‡∏¢‡πà‡∏≤‡∏á‡∏û‡∏£‡πâ‡∏≠‡∏°
    print("‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡πÅ‡∏•‡πâ‡∏ß")
else
    print("‡πÑ‡∏°‡πà‡∏™‡∏≤‡∏°‡∏≤‡∏£‡∏ñ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏Å‡∏≤‡∏£‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡πÑ‡∏î‡πâ‡πÄ‡∏ô‡∏∑‡πà‡∏≠‡∏á‡∏à‡∏≤‡∏Å‡∏ö‡∏≤‡∏á‡∏™‡πà‡∏ß‡∏ô‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡∏û‡∏£‡πâ‡∏≠‡∏°")
end

-- üìå ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡πÅ‡∏°‡∏û‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£
local function teleportToMap(mapID)
    local success, errorMsg = pcall(function()
        game:GetService("TeleportService"):Teleport(mapID, game:GetService("Players").LocalPlayer)
    end)
    
    -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤‡∏Å‡∏≤‡∏£‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à‡∏´‡∏£‡∏∑‡∏≠‡πÑ‡∏°‡πà
    if not success then
        warn("‡∏Å‡∏≤‡∏£‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏•‡πâ‡∏°‡πÄ‡∏´‡∏•‡∏ß: " .. errorMsg)
    else
        print("‡∏Å‡∏≤‡∏£‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡πÅ‡∏°‡∏û " .. mapID .. " ‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à!")
    end
end
-----------------------------------------

-- üìå ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡πÅ‡∏°‡∏û‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£
local function teleportToMap(mapID)
    local success, errorMsg = pcall(function()
        game:GetService("TeleportService"):Teleport(mapID, game:GetService("Players").LocalPlayer)
    end)
    
    -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤‡∏Å‡∏≤‡∏£‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à‡∏´‡∏£‡∏∑‡∏≠‡πÑ‡∏°‡πà
    if not success then
        warn("‡∏Å‡∏≤‡∏£‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏•‡πâ‡∏°‡πÄ‡∏´‡∏•‡∏ß: " .. errorMsg)
    else
        print("‡∏Å‡∏≤‡∏£‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡πÅ‡∏°‡∏û " .. mapID .. " ‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à!")
    end
end

-- üìå ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏à‡∏±‡∏î‡∏Å‡∏≤‡∏£‡∏Å‡∏≤‡∏£‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡πÅ‡∏°‡∏û
local function handleMapTransition(currentMapID)
    if currentMapID ~= 110577167676254 then
        teleportToMap(110577167676254)
    end
end

-- üìå ‡∏£‡∏≠‡∏à‡∏ô‡∏Å‡∏ß‡πà‡∏≤ Player ‡∏à‡∏∞‡∏ñ‡∏π‡∏Å‡πÄ‡∏û‡∏¥‡πà‡∏°‡πÄ‡∏Ç‡πâ‡∏≤‡∏°‡∏≤‡πÉ‡∏ô‡πÄ‡∏Å‡∏°
game:GetService("Players").PlayerAdded:Connect(function(player)
    -- ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÄ‡∏Ç‡πâ‡∏≤‡∏°‡∏≤‡πÅ‡∏•‡πâ‡∏ß ‡πÉ‡∏´‡πâ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö PlaceId ‡πÅ‡∏•‡∏∞‡∏ó‡∏≥‡∏Å‡∏≤‡∏£‡∏ß‡∏≤‡∏£‡πå‡∏õ
    handleMapTransition(game.PlaceId)
end)
------- üìå ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏ä‡πá‡∏Ñ Map ID
local function checkMapID()
    local currentMapID = game.PlaceId
    for _, id in ipairs(mapIDs) do
        if currentMapID == id then
            return true
        end
    end
    return false
end

-- üìå ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏ä‡πá‡∏Ñ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡∏≠‡∏¢‡∏π‡πà‡πÉ‡∏Å‡∏•‡πâ‡πÄ‡∏Ñ‡∏µ‡∏¢‡∏á
local function checkNearbyPlayers()
    local detectionRadius = 10000
    local excludedPlayers = {"zMaxikax", "DarkFalcon_Z"}

    for _, otherPlayer in pairs(Players:GetPlayers()) do  -- ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏à‡∏≤‡∏Å game.Players ‡πÄ‡∏õ‡πá‡∏ô Players
        if otherPlayer ~= player and not table.find(excludedPlayers, string.lower(otherPlayer.Name)) and otherPlayer.Character and otherPlayer.Character:FindFirstChild("HumanoidRootPart") then
            local otherCharacter = otherPlayer.Character
            local distance = (player.Character.HumanoidRootPart.Position - otherCharacter.HumanoidRootPart.Position).Magnitude

            if distance <= detectionRadius then
                warn("üö® ‡∏û‡∏ö‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÉ‡∏Å‡∏•‡πâ‡πÄ‡∏Ñ‡∏µ‡∏¢‡∏á! ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏¢‡∏±‡∏á Safe Zone...")
                TeleportService:Teleport(133649758958568, player)
                return true
            end
        end
    end
    return false
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏ä‡πá‡∏Ñ‡πÄ‡∏•‡∏∑‡∏≠‡∏î‡∏î‡∏¥‡∏à‡∏¥‡∏°‡πà‡∏≠‡∏ô‡πÅ‡∏•‡∏∞‡πÄ‡∏£‡∏µ‡∏¢‡∏Å‡∏Å‡∏•‡∏±‡∏ö
local function checkPetHealth()
    local humanoidRootPart = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
    if humanoidRootPart then
        local pet = humanoidRootPart:FindFirstChild("pet1")  -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤‡∏°‡∏µ pet1 ‡∏≠‡∏¢‡∏π‡πà‡πÉ‡∏ô HumanoidRootPart ‡∏´‡∏£‡∏∑‡∏≠‡πÑ‡∏°‡πà
        if pet and pet:FindFirstChild("Stats") and pet.Stats:FindFirstChild("Health") then
            local healthStat = pet.Stats.Health
            local maxHealthStat = pet.Stats.HealthMax

            if healthStat.Value / maxHealthStat.Value < 0.6 then
                print("‚ö† ‡πÄ‡∏•‡∏∑‡∏≠‡∏î‡∏î‡∏¥‡∏à‡∏¥‡∏°‡πà‡∏≠‡∏ô‡∏ï‡πà‡∏≥‡∏Å‡∏ß‡πà‡∏≤ 60% ‚Üí ‡πÄ‡∏£‡∏µ‡∏¢‡∏Å‡∏î‡∏¥‡∏à‡∏¥‡∏°‡πà‡∏≠‡∏ô‡∏Å‡∏•‡∏±‡∏ö")
                
                -- ‡πÄ‡∏£‡∏µ‡∏¢‡∏Å‡∏î‡∏¥‡∏à‡∏¥‡∏°‡πà‡∏≠‡∏ô‡∏Å‡∏•‡∏±‡∏ö
                for i = 1, 5 do
                    local args = { [1] = "9566301740688870.6489878", [2] = false }
                    game:GetService("Players").LocalPlayer.PlayerGui.CombatClient.CallDigimon:InvokeServer(unpack(args))
                    wait(0.1)
                end
                
                -- ‡∏£‡∏≠‡∏à‡∏ô‡∏Å‡∏ß‡πà‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏î‡∏à‡∏∞‡πÄ‡∏ï‡πá‡∏° 100% ‡∏Å‡πà‡∏≠‡∏ô‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏ï‡πà‡∏≠
                repeat
                    wait(0.1)  -- ‡πÄ‡∏ä‡πá‡∏Ñ‡∏ó‡∏∏‡∏Å‡πÜ 0.1 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ
                until healthStat.Value / maxHealthStat.Value >= 1  -- ‡πÄ‡∏ä‡πá‡∏Ñ‡∏ß‡πà‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏î‡πÄ‡∏ï‡πá‡∏° 100% ‡∏´‡∏£‡∏∑‡∏≠‡∏¢‡∏±‡∏á
                
                print("üíØ ‡πÄ‡∏•‡∏∑‡∏≠‡∏î‡∏î‡∏¥‡∏à‡∏¥‡∏°‡πà‡∏≠‡∏ô‡πÄ‡∏ï‡πá‡∏°‡πÅ‡∏•‡πâ‡∏ß ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏ï‡πà‡∏≠...")
                return true -- ‡∏£‡∏∞‡∏ö‡∏∏‡∏ß‡πà‡∏≤‡πÉ‡∏´‡πâ‡∏´‡∏¢‡∏∏‡∏î‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏ä‡∏±‡πà‡∏ß‡∏Ñ‡∏£‡∏≤‡∏ß‡∏à‡∏ô‡∏Å‡∏ß‡πà‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏î‡∏à‡∏∞‡πÄ‡∏ï‡πá‡∏°
            end
        end
    end
    return false -- ‡∏ñ‡πâ‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏î‡∏°‡∏≤‡∏Å‡∏Å‡∏ß‡πà‡∏≤ 60% ‡πÉ‡∏´‡πâ‡∏ó‡∏≥‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏ï‡πà‡∏≠
end

-- üìå ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏Ñ‡∏•‡∏¥‡∏Å‡πÅ‡∏•‡∏∞‡πÉ‡∏ä‡πâ‡∏™‡∏Å‡∏¥‡∏•
local function clickDetectors()
    for _, part in pairs(workspace.Collect:GetChildren()) do
        if part:IsA("Part") and part:FindFirstChild("Health") and part.Health.Value > 0 then
            fireclickdetector(part:FindFirstChildWhichIsA("ClickDetector", true))
        end
    end
end

local function useSkill()
    local currentMapID = game.PlaceId
    local args
    
    if currentMapID == 110577167676254 then
        args = {
            [1] = 68889,
            [2] = 14,
            [3] = "3886291740644595.1340752",
            [4] = false
        }
        game:GetService("Players").LocalPlayer.PlayerGui.CombatClient.Skill3:InvokeServer(unpack(args))
    else
        args = {
            [1] = 4617,
            [2] = 7,
            [3] = "8359791740798128.694923",
            [4] = false
        }
        game:GetService("Players").LocalPlayer.PlayerGui.CombatClient.Skill2:InvokeServer(unpack(args))
    end
end

-- üìå ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏õ‡∏≤‡∏£‡πå‡∏ï‡∏µ‡πâ‡πÅ‡∏•‡∏∞‡∏£‡∏µ‡∏´‡πâ‡∏≠‡∏á
local function createPartyAndRestart()
    local args1 = { [1] = "createParty", [2] = "zMaxikax's Party", [3] = 4 }
    ReplicatedStorage.PartySystemRE:FireServer(unpack(args1))
    task.wait(1)

    local args2 = { [1] = "startParty", [2] = "zMaxikax's Party" }
    ReplicatedStorage.PartySystemRE:FireServer(unpack(args2))
    print("‡∏£‡∏µ‡∏´‡πâ‡∏≠‡∏á‡πÅ‡∏•‡πâ‡∏ß‡πÅ‡∏•‡∏∞‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏õ‡∏≤‡∏£‡πå‡∏ï‡∏µ‡πâ‡πÉ‡∏´‡∏°‡πà")
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏≠‡∏≠‡πÇ‡∏ï‡πâ‡∏ü‡∏≤‡∏£‡πå‡∏°
local function autoFarm()
    if not checkMapID() then return end  -- ‡∏ñ‡πâ‡∏≤‡πÑ‡∏°‡πà‡∏ï‡∏£‡∏á‡∏Å‡∏±‡∏ö Map ID ‡∏ó‡∏µ‡πà‡∏Å‡∏≥‡∏´‡∏ô‡∏î‡∏à‡∏∞‡πÑ‡∏°‡πà‡∏ó‡∏≥‡∏Å‡∏≤‡∏£‡∏ü‡∏≤‡∏£‡πå‡∏°

    local startTime = tick()
    local monsterFound = false

    while true do
        monsterFound = false  -- ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏Ñ‡πà‡∏≤‡∏Å‡πà‡∏≠‡∏ô‡πÉ‡∏ô‡πÅ‡∏ï‡πà‡∏•‡∏∞‡∏£‡∏≠‡∏ö‡∏Ç‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡∏ß‡∏ô loop

        -- ‡πÄ‡∏ä‡πá‡∏Ñ‡∏´‡∏≤‡∏°‡∏≠‡∏ô‡∏™‡πÄ‡∏ï‡∏≠‡∏£‡πå‡πÉ‡∏ô Workspace.Collect
        for _, part in pairs(workspace.Collect:GetChildren()) do
            if part:IsA("Part") and part:FindFirstChild("Health") then
                monsterFound = true
                local pet = player.Character:FindFirstChild("HumanoidRootPart") and player.Character.HumanoidRootPart.pet1
                local playerDigimonHealth = pet and pet.Stats.Health

                -- ‡πÄ‡∏ä‡πá‡∏Ñ‡πÄ‡∏•‡∏∑‡∏≠‡∏î‡∏î‡∏¥‡∏à‡∏¥‡∏°‡πà‡∏≠‡∏ô
                if playerDigimonHealth and playerDigimonHealth.Value < 60 then
                    -- ‡πÄ‡∏£‡∏µ‡∏¢‡∏Å‡∏î‡∏¥‡∏à‡∏¥‡∏°‡πà‡∏≠‡∏ô‡∏Å‡∏•‡∏±‡∏ö
                    for i = 1, 5 do
                        local args = {
                            [1] = "9566301740688870.6489878",
                            [2] = false
                        }
                        game:GetService("Players").LocalPlayer.PlayerGui.CombatClient.CallDigimon:InvokeServer(unpack(args))
                        wait(0.1)
                    end
                    task.wait(18)
                    print("‚è≥ ‡∏Ñ‡∏£‡∏ö 18 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ‡πÅ‡∏•‡πâ‡∏ß ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏ï‡πà‡∏≠...")
                    break  -- ‡∏≠‡∏≠‡∏Å‡∏à‡∏≤‡∏Å‡∏Å‡∏≤‡∏£‡∏ß‡∏ô‡∏•‡∏π‡∏õ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÄ‡∏£‡∏µ‡∏¢‡∏Å‡∏î‡∏¥‡∏à‡∏¥‡∏°‡πà‡∏≠‡∏ô‡∏Å‡∏•‡∏±‡∏ö
                else
                    -- ‡∏™‡πà‡∏á‡∏î‡∏¥‡∏à‡∏¥‡∏°‡πà‡∏≠‡∏ô‡∏≠‡∏≠‡∏Å‡πÑ‡∏õ‡πÇ‡∏à‡∏°‡∏ï‡∏µ‡πÅ‡∏•‡∏∞‡πÉ‡∏ä‡πâ‡∏™‡∏Å‡∏¥‡∏•
                    clickDetectors()
                    useSkill()
                end
                break
            end
        end

        -- ‡∏ñ‡πâ‡∏≤‡∏´‡∏≤‡∏°‡∏≠‡∏ô‡∏™‡πÄ‡∏ï‡∏≠‡∏£‡πå‡πÑ‡∏°‡πà‡πÑ‡∏î‡πâ‡∏†‡∏≤‡∏¢‡πÉ‡∏ô 10 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ‡πÉ‡∏´‡πâ‡∏£‡∏µ‡∏´‡πâ‡∏≠‡∏á
        if not monsterFound then
            if tick() - startTime > 10 then
                warn("‡πÑ‡∏°‡πà‡∏û‡∏ö‡∏°‡∏≠‡∏ô‡∏™‡πÄ‡∏ï‡∏≠‡∏£‡πå ‡∏£‡∏µ‡∏´‡πâ‡∏≠‡∏á...")
                TeleportService:Teleport(110577167676254, player) -- ‡∏¢‡πâ‡∏≤‡∏¢‡πÑ‡∏õ‡πÅ‡∏°‡∏û‡πÄ‡∏î‡∏¥‡∏°
                break
            end
        else
            -- ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï startTime ‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡∏ó‡∏µ‡πà‡∏û‡∏ö‡∏°‡∏≠‡∏ô‡∏™‡πÄ‡∏ï‡∏≠‡∏£‡πå
            startTime = tick()
        end

        -- ‡πÄ‡∏ä‡πá‡∏Ñ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡πÉ‡∏Å‡∏•‡πâ‡πÄ‡∏Ñ‡∏µ‡∏¢‡∏á‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏£‡∏±‡πâ‡∏á
        if checkNearbyPlayers() then
            break
        end
        
        -- ‡πÄ‡∏ä‡πá‡∏Ñ‡πÄ‡∏•‡∏∑‡∏≠‡∏î‡∏î‡∏¥‡∏à‡∏¥‡∏°‡πà‡∏≠‡∏ô
        if checkPetHealth() then
            -- ‡∏´‡∏≤‡∏Å‡πÄ‡∏•‡∏∑‡∏≠‡∏î‡∏î‡∏¥‡∏à‡∏¥‡∏°‡πà‡∏≠‡∏ô‡∏ï‡πà‡∏≥‡∏Å‡∏ß‡πà‡∏≤ 60% ‡πÅ‡∏•‡∏∞‡πÄ‡∏£‡∏µ‡∏¢‡∏Å‡∏Å‡∏•‡∏±‡∏ö‡πÅ‡∏•‡πâ‡∏ß ‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏Å‡∏•‡∏±‡∏ö‡∏°‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πà‡∏≠
            continue
        end

        wait(0.1) -- ‡∏õ‡∏£‡∏±‡∏ö‡πÄ‡∏ß‡∏•‡∏≤‡∏ï‡∏≤‡∏°‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£
    end
end

-- ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏≠‡∏≠‡πÇ‡∏ï‡πâ‡∏ü‡∏≤‡∏£‡πå‡∏°‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÅ‡∏°‡∏û‡∏ñ‡∏π‡∏Å‡∏ï‡πâ‡∏≠‡∏á
if checkMapID() then
    autoFarm()
end
