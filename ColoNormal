local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Players = game:GetService("Players")
local TeleportService = game:GetService("TeleportService")
local HttpService = game:GetService("HttpService")
local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait() -- ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏°‡∏µ‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

local collectFolder = workspace:FindFirstChild("Collect")
local isPartyCreated = false
local isFarming = false  -- ‡πÄ‡∏ä‡πá‡∏Ñ‡∏Å‡∏≤‡∏£‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô‡∏ü‡∏≤‡∏£‡πå‡∏°
local isScriptRunning = true -- ‡πÄ‡∏ä‡πá‡∏Ñ‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞‡∏Å‡∏≤‡∏£‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡∏Ç‡∏≠‡∏á‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå
local firstTP = true  -- ‡πÉ‡∏ä‡πâ‡πÄ‡∏ä‡πá‡∏Å‡∏ß‡πà‡∏≤‡∏Å‡∏≥‡∏•‡∏±‡∏á TP ‡πÑ‡∏õ‡∏´‡∏≤‡∏°‡∏≠‡∏ô‡∏ï‡∏±‡∏ß‡πÅ‡∏£‡∏Å

-- üìå ‡∏î‡∏µ‡πÄ‡∏•‡∏¢‡πå 10 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ‡∏Å‡πà‡∏≠‡∏ô‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå
task.wait(10)

local camera = game.Workspace.CurrentCamera

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ó‡∏µ‡πà‡πÉ‡∏ä‡πâ‡∏ã‡∏π‡∏°‡∏´‡∏ô‡πâ‡∏≤‡∏à‡∏≠‡∏≠‡∏≠‡∏Å
local function zoomOut()
camera.FieldOfView = 100 -- ‡∏Å‡∏≥‡∏´‡∏ô‡∏î FOV ‡∏ó‡∏µ‡πà‡∏Å‡∏ß‡πâ‡∏≤‡∏á‡∏Ç‡∏∂‡πâ‡∏ô (‡∏Ñ‡πà‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô‡∏Ñ‡∏∑‡∏≠ 70)
end

-- ‡∏£‡∏±‡∏ô‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ã‡∏π‡∏°‡∏≠‡∏≠‡∏Å‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡πÄ‡∏î‡∏µ‡∏¢‡∏ß‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÇ‡∏´‡∏•‡∏î‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå
zoomOut()

-- üìå ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤‡∏≠‡∏¢‡∏π‡πà‡πÉ‡∏ô MAPID ‡∏ó‡∏µ‡πà‡∏£‡∏∞‡∏ö‡∏∏‡∏´‡∏£‡∏∑‡∏≠‡πÑ‡∏°‡πà
local function isInMapID(mapID)
    local currentMapID = game.PlaceId
    return currentMapID == mapID
end

-- üìå ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏õ‡∏≤‡∏£‡πå‡∏ï‡∏µ‡πâ
local function createAndStartParty()
    if not isPartyCreated then
        print("üö® ‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏°‡∏≠‡∏ô / ‡∏°‡∏≠‡∏ô‡πÄ‡∏´‡∏•‡∏∑‡∏≠‡πÄ‡∏ó‡πà‡∏≤‡πÄ‡∏î‡∏¥‡∏° 5 ‡∏ß‡∏¥ ‚Üí ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏õ‡∏≤‡∏£‡πå‡∏ï‡∏µ‡πâ...")
        local args1 = { [1] = "createParty", [2] = "zMaxikax's Party", [3] = 4 }
        ReplicatedStorage.PartySystemRE:FireServer(unpack(args1))
        task.wait(1)

        local args2 = { [1] = "startParty", [2] = "zMaxikax's Party" }
        ReplicatedStorage.PartySystemRE:FireServer(unpack(args2))

        isPartyCreated = true
    else
        print("‚úÖ ‡∏õ‡∏≤‡∏£‡πå‡∏ï‡∏µ‡πâ‡∏ñ‡∏π‡∏Å‡∏™‡∏£‡πâ‡∏≤‡∏á‡πÑ‡∏õ‡πÅ‡∏•‡πâ‡∏ß ‡πÑ‡∏°‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏™‡∏£‡πâ‡∏≤‡∏á‡πÉ‡∏´‡∏°‡πà")
    end
end

-- üìå ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏°‡∏≠‡∏ô‡∏ó‡∏µ‡πà TP ‡∏°‡∏≤‡∏´‡∏≤‡∏¢‡πÑ‡∏õ‡∏´‡∏£‡∏∑‡∏≠‡∏ï‡∏≤‡∏¢‡∏´‡∏£‡∏∑‡∏≠‡∏¢‡∏±‡∏á
local function checkBaseParts()
    if not collectFolder then return end

    -- ‡∏´‡∏≤ Part ‡∏Ç‡∏≠‡∏á‡∏°‡∏≠‡∏ô‡∏ó‡∏µ‡πà‡∏ñ‡∏π‡∏Å TP ‡πÑ‡∏õ
    local initialPart = collectFolder:GetChildren()[1]  -- ‡πÄ‡∏≠‡∏≤‡πÄ‡∏û‡∏µ‡∏¢‡∏á Part ‡∏ï‡∏±‡∏ß‡πÅ‡∏£‡∏Å‡∏°‡∏≤‡πÄ‡∏Å‡πá‡∏ö‡πÑ‡∏ß‡πâ
    if not initialPart then return end  -- ‡∏ñ‡πâ‡∏≤‡πÑ‡∏°‡πà‡∏û‡∏ö Part ‡πÉ‡∏´‡πâ‡∏´‡∏¢‡∏∏‡∏î

    print("üìä ‡∏°‡∏≠‡∏ô‡∏ó‡∏µ‡πà TP ‡πÑ‡∏õ: " .. initialPart.Name)

    task.wait(30)  -- ‡∏£‡∏≠ 25 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ

    -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤ Part ‡∏Ç‡∏≠‡∏á‡∏°‡∏≠‡∏ô‡∏ó‡∏µ‡πà TP ‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡∏≠‡∏¢‡∏π‡πà‡πÉ‡∏ô collectFolder ‡∏´‡∏£‡∏∑‡∏≠‡πÑ‡∏°‡πà
    if not collectFolder:FindFirstChild(initialPart.Name) then
        print("‚úÖ ‡∏°‡∏≠‡∏ô‡∏ï‡∏±‡∏ß‡∏ó‡∏µ‡πà TP ‡∏°‡∏≤‡∏´‡∏≤‡∏¢‡πÑ‡∏õ‡∏´‡∏£‡∏∑‡∏≠‡∏ï‡∏≤‡∏¢‡πÅ‡∏•‡πâ‡∏ß ‚Üí ‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏ï‡πà‡∏≠‡πÑ‡∏î‡πâ")
    else
        print("‚ö† ‡∏°‡∏≠‡∏ô‡∏ï‡∏±‡∏ß‡∏ó‡∏µ‡πà TP ‡∏°‡∏≤‡∏¢‡∏±‡∏á‡∏≠‡∏¢‡∏π‡πà ‚Üí ‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏ï‡∏≤‡∏¢...")

        -- ‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏ï‡∏≤‡∏¢‡πÇ‡∏î‡∏¢‡∏Å‡∏≤‡∏£‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤ Health ‡πÄ‡∏õ‡πá‡∏ô 0
        local character = player.Character
        if character and character:FindFirstChild("Humanoid") then
            character.Humanoid.Health = 0  
        end
    end
end

-- üìå ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÉ‡∏Å‡∏•‡πâ‡πÄ‡∏Ñ‡∏µ‡∏¢‡∏á
local function checkNearbyPlayers()
    local detectionRadius = 10000 
    local excludedPlayers = {"zMaxikax", "DarkFalcon_Z"} -- ‡∏ä‡∏∑‡πà‡∏≠‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡πÑ‡∏°‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡πÉ‡∏´‡πâ‡∏ï‡∏£‡∏ß‡∏à‡∏à‡∏±‡∏ö

    for _, otherPlayer in pairs(Players:GetPlayers()) do
        -- ‡πÄ‡∏ä‡πá‡∏Ñ‡∏ß‡πà‡∏≤‡πÑ‡∏°‡πà‡πÉ‡∏ä‡πà‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ï‡∏±‡∏ß‡πÄ‡∏≠‡∏á‡πÅ‡∏•‡∏∞‡πÑ‡∏°‡πà‡πÉ‡∏ä‡πà‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡∏≠‡∏¢‡∏π‡πà‡πÉ‡∏ô‡∏•‡∏¥‡∏™‡∏ï‡πå‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡∏¢‡∏Å‡πÄ‡∏ß‡πâ‡∏ô
        if otherPlayer ~= player and not table.find(excludedPlayers, otherPlayer.Name) and otherPlayer.Character and otherPlayer.Character:FindFirstChild("HumanoidRootPart") then
            local otherCharacter = otherPlayer.Character
            local distance = (character.HumanoidRootPart.Position - otherCharacter.HumanoidRootPart.Position).Magnitude

            if distance <= detectionRadius then
                warn("üö® ‡∏û‡∏ö‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÉ‡∏Å‡∏•‡πâ‡πÄ‡∏Ñ‡∏µ‡∏¢‡∏á! ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏¢‡∏±‡∏á Safe Zone...")
                TeleportService:Teleport(133649758958568, player)  
                return true  
            end
        end
    end
    return false 
end


-- üìå ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÉ‡∏ä‡πâ‡∏™‡∏Å‡∏¥‡∏•‡∏£‡∏±‡∏ß‡πÜ
local function useSkillRapidly()
    while true do
        local args1 = {
            [1] = 68889,
            [2] = 14,
            [3] = "3886291740644595.1340752",
            [4] = false
        }

        local args2 = {
            [1] = 68889,
            [2] = 14,
            [3] = "6969441740738959.653765",
            [4] = false
        }

        local combatClient = game:GetService("Players").LocalPlayer.PlayerGui.CombatClient.Skill3
        combatClient:InvokeServer(unpack(args1))
        combatClient:InvokeServer(unpack(args2))

        task.wait(0.1) -- ‡∏´‡∏ô‡πà‡∏ß‡∏á‡πÄ‡∏ß‡∏•‡∏≤ 0.1 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ
    end
end


-- üìå ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏ä‡πá‡∏Ñ‡πÄ‡∏•‡∏∑‡∏≠‡∏î‡∏î‡∏¥‡∏à‡∏¥‡∏°‡πà‡∏≠‡∏ô
local function checkPetHealthAndTeleport()
    local pet = workspace:WaitForChild("zMaxikax").HumanoidRootPart.pet1
    local healthStat = pet.Stats.Health
    local maxHealthStat = pet.Stats.HealthMax
    local teleportPosition = CFrame.new(467.62384033203125, 315.87823486328125, -109.36079406738281)

    if healthStat and maxHealthStat then
        -- ‡πÄ‡∏ä‡πá‡∏Ñ‡∏ß‡πà‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏î‡∏Ç‡∏≠‡∏á‡∏î‡∏¥‡∏à‡∏¥‡∏°‡πà‡∏≠‡∏ô‡∏ï‡πà‡∏≥‡∏Å‡∏ß‡πà‡∏≤ 99%
        if healthStat.Value / maxHealthStat.Value < 0.6 then
            print("‚ö† ‡πÄ‡∏•‡∏∑‡∏≠‡∏î‡∏î‡∏¥‡∏à‡∏¥‡∏°‡πà‡∏≠‡∏ô‡∏ï‡πà‡∏≥‡∏Å‡∏ß‡πà‡∏≤ 99% ‚Üí TP ‡πÑ‡∏õ‡πÄ‡∏ã‡∏ü‡πÇ‡∏ã‡∏ô‡πÅ‡∏•‡∏∞‡πÄ‡∏£‡∏µ‡∏¢‡∏Å‡∏î‡∏¥‡∏à‡∏¥‡∏°‡πà‡∏≠‡∏ô‡∏Å‡∏•‡∏±‡∏ö")
            
            -- TP ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÑ‡∏õ‡∏ó‡∏µ‡πà‡∏à‡∏∏‡∏î‡∏ó‡∏µ‡πà‡∏Å‡∏≥‡∏´‡∏ô‡∏î
            player.Character:SetPrimaryPartCFrame(teleportPosition)

            -- ‡πÄ‡∏£‡∏µ‡∏¢‡∏Å‡∏î‡∏¥‡∏à‡∏¥‡∏°‡πà‡∏≠‡∏ô‡∏Å‡∏•‡∏±‡∏ö 5 ‡∏Ñ‡∏£‡∏±‡πâ‡∏á
            for i = 1, 5 do
                local args = {
                    [1] = "9566301740688870.6489878",
                    [2] = false
                }
                game:GetService("Players").LocalPlayer.PlayerGui.CombatClient.CallDigimon:InvokeServer(unpack(args))
                wait(0.1)  -- ‡∏£‡∏≠ 0.5 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ‡∏£‡∏∞‡∏´‡∏ß‡πà‡∏≤‡∏á‡∏Å‡∏≤‡∏£‡πÄ‡∏£‡∏µ‡∏¢‡∏Å 
            end

            -- ‡∏£‡∏≠ 10 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ‡∏´‡∏•‡∏±‡∏á‡∏à‡∏≤‡∏Å‡πÄ‡∏£‡∏µ‡∏¢‡∏Å‡∏î‡∏¥‡∏à‡∏¥‡∏°‡πà‡∏≠‡∏ô
            task.wait(18)

            -- ‡∏´‡∏•‡∏±‡∏á‡∏à‡∏≤‡∏Å‡∏£‡∏≠ 10 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ‡πÉ‡∏´‡πâ‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏ï‡πà‡∏≠‡πÑ‡∏õ
            return false -- ‡∏™‡πà‡∏á‡∏Ñ‡πà‡∏≤‡∏Å‡∏•‡∏±‡∏ö‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÑ‡∏°‡πà‡πÉ‡∏´‡πâ‡∏´‡∏¢‡∏∏‡∏î‡∏ü‡∏≤‡∏£‡πå‡∏°
        end
    end
    return false
end



-- üìå ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô Auto Farm
local function autoFarm()
    local mapIDs = {86392425558311, 80299472659017, 120958789962854, 76011326497329, 110577167676254}
    local isInValidMap = false

    for _, mapID in ipairs(mapIDs) do
        if isInMapID(mapID) then
            isInValidMap = true
            break
        end
    end

    if not isInValidMap then
        print("üö® ‡πÑ‡∏°‡πà‡∏≠‡∏¢‡∏π‡πà‡πÉ‡∏ô MAP ‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£ ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏Å‡∏≤‡∏£‡∏ü‡∏≤‡∏£‡πå‡∏°‡πÑ‡∏°‡πà‡πÑ‡∏î‡πâ")
        return
    end

    if not collectFolder then
        warn("‚ùå ‡πÑ‡∏°‡πà‡∏û‡∏ö‡πÇ‡∏ü‡∏•‡πÄ‡∏î‡∏≠‡∏£‡πå Collect!")
        return
    end

    print("üìç ‡∏û‡∏ö‡πÇ‡∏ü‡∏•‡πÄ‡∏î‡∏≠‡∏£‡πå Collect! ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ü‡∏≤‡∏£‡πå‡∏°...")

    local startTime = tick()
    
    if not isFarming then
        isFarming = true  
        task.spawn(useSkillRapidly)  -- ‡πÄ‡∏£‡∏µ‡∏¢‡∏Å‡πÉ‡∏ä‡πâ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÉ‡∏ä‡πâ‡∏™‡∏Å‡∏¥‡∏•‡∏£‡∏±‡∏ß‡πÜ
    end

    while true do
        if not isScriptRunning then
            print("‚õî ‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡∏´‡∏¢‡∏∏‡∏î‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡πÅ‡∏•‡πâ‡∏ß")
            return 
        end

        if checkNearbyPlayers() then
            print("‚ö† ‡∏´‡∏¢‡∏∏‡∏î‡∏Å‡∏≤‡∏£‡∏ü‡∏≤‡∏£‡πå‡∏°‡πÄ‡∏ô‡∏∑‡πà‡∏≠‡∏á‡∏à‡∏≤‡∏Å‡∏°‡∏µ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÉ‡∏Å‡∏•‡πâ‡πÄ‡∏Ñ‡∏µ‡∏¢‡∏á")
            return 
        end

        -- ‡πÄ‡∏ä‡πá‡∏Ñ‡πÄ‡∏•‡∏∑‡∏≠‡∏î‡∏î‡∏¥‡∏à‡∏¥‡∏°‡πà‡∏≠‡∏ô‡∏Å‡πà‡∏≠‡∏ô TP ‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡∏°‡∏≠‡∏ô
        if checkPetHealthAndTeleport() then
            return
        end

        local foundPart = false

        for _, part in ipairs(collectFolder:GetChildren()) do
            if part:IsA("Part") then
                humanoidRootPart.CFrame = part.CFrame
                print("‚öî TP ‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡∏°‡∏≠‡∏ô: " .. tostring(part.CFrame))

                foundPart = true
                startTime = tick() 
                task.wait(0.5)

                -- üü¢ ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏°‡∏≠‡∏ô‡∏´‡∏•‡∏±‡∏á TP ‡πÑ‡∏õ‡∏´‡∏≤‡∏ï‡∏±‡∏ß‡πÅ‡∏£‡∏Å
                if firstTP then
                    task.spawn(checkBaseParts) -- ‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡∏£‡∏±‡∏ô‡πÅ‡∏¢‡∏Å‡∏à‡∏≤‡∏Å loop
                    firstTP = false
                end

                break
            end
        end

        local timeElapsed = tick() - startTime

        -- üî¥ ‡∏ñ‡πâ‡∏≤‡πÑ‡∏°‡πà‡πÄ‡∏à‡∏≠‡∏°‡∏≠‡∏ô‡πÉ‡∏ô 8 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ ‚Üí ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏õ‡∏≤‡∏£‡πå‡∏ï‡∏µ‡πâ
        if not foundPart and timeElapsed >= 5 then
            createAndStartParty()
            startTime = tick()
        end

        task.wait(0.5)
    end
end

-- üöÄ ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ó‡∏µ‡πà‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÄ‡∏Å‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà
local function onCharacterAdded(newCharacter)
    character = newCharacter
    humanoidRootPart = character:WaitForChild("HumanoidRootPart")
    
    isPartyCreated = false
    firstTP = false -- ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏Ñ‡πà‡∏≤ ‡πÉ‡∏´‡πâ checkBaseParts ‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡∏´‡∏•‡∏±‡∏á‡∏ï‡∏≤‡∏¢
    task.wait(1) -- ‡∏£‡∏≠ 1 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ‡∏Å‡πà‡∏≠‡∏ô‡πÄ‡∏£‡∏¥‡πà‡∏° Auto Farm ‡∏≠‡∏µ‡∏Å‡∏Ñ‡∏£‡∏±‡πâ‡∏á
    autoFarm()
    
    -- ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÉ‡∏ä‡πâ‡∏™‡∏Å‡∏¥‡∏•‡∏£‡∏±‡∏ß‡πÜ
    task.spawn(useSkillRapidly)
end

-- ‡∏™‡∏°‡∏±‡∏Ñ‡∏£‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÉ‡∏´‡πâ‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÄ‡∏Å‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà
player.CharacterAdded:Connect(onCharacterAdded)

-- ‡∏ñ‡πâ‡∏≤‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏°‡∏µ‡∏≠‡∏¢‡∏π‡πà‡πÅ‡∏•‡πâ‡∏ß ‡πÉ‡∏´‡πâ‡πÄ‡∏£‡∏µ‡∏¢‡∏Å onCharacterAdded ‡∏ó‡∏±‡∏ô‡∏ó‡∏µ
if player.Character then
    onCharacterAdded(player.Character)
end

-- ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô Auto Farm ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÄ‡∏£‡∏¥‡πà‡∏°‡πÄ‡∏Å‡∏°
autoFarm()

-- ‡πÄ‡∏ä‡πá‡∏Ñ‡πÄ‡∏•‡∏∑‡∏≠‡∏î‡∏î‡∏¥‡∏à‡∏¥‡∏°‡πà‡∏≠‡∏ô
task.spawn(function()
    while true do
        if checkPetHealthAndTeleport() then
            return
        end
        task.wait(0.1) -- ‡πÄ‡∏ä‡πá‡∏Ñ‡πÄ‡∏•‡∏∑‡∏≠‡∏î‡∏ó‡∏∏‡∏Å 1 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ
    end
end)
